{
    "name": "margonemski-harvester",
    "author": "mikalsqwe",
    "description": "Można se pobrać rzeczy z tej strony -> https://www.margonem.pl",
    "license": "MIT",
    "version": "0.1.0",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "keywords": [
        "Margonem",
        "świry margonemskie"
    ],
    "homepage": "https://margonemski-harvester.vercel.app/pl/introduction",
    "repository": {
        "type": "git",
        "url": "https://github.com/KamilGrocholski/margonemski-harvester"
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "release": "pnpm run build && changeset publish",
        "lint": "tsc",
        "format": "prettier --write src",
        "test": "jest",
        "test:serially": "jest -i"
    },
    "files": [
        "src",
        "dist",
        "README.md",
        "package.json"
    ],
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "default": "./dist/index.js"
        }
    },
    "devDependencies": {
        "@changesets/cli": "^2.26.0",
        "@types/jest": "^29.5.0",
        "jest": "^29.5.0",
        "prettier": "2.8.7",
        "ts-jest": "^29.1.0",
        "tsup": "^6.5.0",
        "typescript": "^4.9.4"
    },
    "dependencies": {
        "axios": "^1.3.4",
        "cheerio": "^1.0.0-rc.12",
        "zod": "^3.21.4"
    }
}
